# DevPod Dashboard configuration
# Copy to .env and adjust values for your deployment

# HTTP Basic Auth credentials
DASHBOARD_USER=admin
DASHBOARD_PASS=changeme

# Listen port
DASHBOARD_PORT=8080

# Kubernetes
KUBECONFIG=/etc/rancher/k3s/k3s.yaml
DASHBOARD_NAMESPACE=devpod

# DevPod system user (the OS user that runs the devpod CLI)
DEVPOD_USER=devpod
DEVPOD_HOME=/home/devpod
